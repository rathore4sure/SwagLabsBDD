<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>light</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>MyConfiguredFWReport</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>My Configured FW Report</reportName>

        <!-- view report without internet -->
        <offlineMode>true</offlineMode>

        <thumbnailForBase64>false</thumbnailForBase64>

        <!-- custom javascript -->
        <js>
        </js>

        <!-- custom styles -->
        <css></css>
    </configuration>
</extentreports>